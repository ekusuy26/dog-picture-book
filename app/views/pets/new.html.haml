= form_with(model: @pet, local: true) do |f|
  = f.label :name
  <br>
  = f.text_field :name
  <br>
  = f.label :age
  <br>
  = f.text_field :age
  <br>
  = f.label :sex_id
  <br>
  = f.collection_check_boxes :sex_id, Sex.all, :id, :name
  <br>
  = f.label :text
  <br>
  = f.text_field :text
  <br>
  = f.label :image
  <br>
  = f.file_field :image
  <br>
  = f.label :category
  <br>
  = f.collection_select :dog_id, @dogs, :id, :name
  <br>
  = f.submit "送信", class: "submit-btn"